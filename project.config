[project]
	description = Source files for the source.android.com site.
[receive]
	requireContributorAgreement = true
[submit]
	action = merge always
	mergeContent = true
[access]
	inheritFrom = Platform-Unrestricted-Projects
[access "refs/heads/master"]
	push = group docs-release
	label-Verified = -1..+1 group AOSP
	label-Verified = -1..+1 group approvers-platform-docs
	label-Verified = -1..+0 group Android Maintainers
	forgeCommitter = group docs-release
	label-Code-Review = -2..+2 group approvers-platform-docs
